{
  "name": "jithin/oscillate",
  "description": "A stock trading helper",
  "type": "project",
  "require": {
    "php": ">=5.4.0",
    "respect/validation": "^1.1",
    "ext-mysqli": "*",
    "ext-json": "*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "com\\oscillate\\": "src/com/oscillate"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\": "test/src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}